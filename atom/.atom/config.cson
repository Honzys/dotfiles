"*":
  Zen:
    softWrap: true
    tabs: "multiple"
  "atom-beautify":
    general:
      _analyticsUserId: "ff4f8bf9-2234-4c89-95eb-b500f761310a"
  "block-cursor":
    global:
      blinkOn: {}
  core:
    disabledPackages: [
      "language-c"
      "language-clojure"
      "language-coffee-script"
      "language-csharp"
      "language-go"
      "language-java"
      "language-make"
      "language-objective-c"
      "language-perl"
      "language-python"
      "language-ruby"
      "language-ruby-on-rails"
      "language-toml"
      "background-tips"
      "welcome"
      "styleguide"
      "metrics"
      "whitespace"
      "angularjs"
      "markdown-preview"
      "linter-jshint"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  "cursor-blink-interval":
    cursorBlinkInterval: 1200
  editor:
    confirmCheckoutHeadRevision: false
    fontFamily: "SourceCodePro-Light"
    invisibles:
      cr: false
      eol: false
    preferredLineLength: 120
    scrollPastEnd: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabLength: 2
  emmet:
    extensionsPath: "~/.emmet"
    formatLineBreaks: false
  "exception-reporting":
    userId: "b9301e45-0712-4ee7-ba90-a7e3f3260eb9"
  "file-icons":
    coloured: false
  linter: {}
  "linter-eslint": {}
  "linter-stylelint": {}
  "linter-ui-default":
    showPanel: true
  "markdown-preview-plus":
    pandocPath: "/usr/local/bin/pandoc"
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
".ini.source":
  editor:
    preferredLineLength: 120
    showIndentGuide: false
